{
    "name": "splat3api-server",
    "version": "1.0.0",
    "main": "dist/app.js",
    "license": "MIT",
    "type": "module",
    "scripts": {
        "dev": "node --no-warnings --loader ts-node/esm --es-module-specifier-resolution=node ./src/app.ts",
        "watch": "nodemon",
        "build": "tsc",
        "ci": "yarn --frozen-lockfile",
        "gen-types": "npx openapi-typescript ./src/types/schema.json --output ./src/types/schema.d.ts"
    },
    "dependencies": {
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "express-rate-limit": "^6.7.0",
        "redis": "^4.6.4",
        "reflect-metadata": "^0.1.13",
        "routing-controllers": "^0.10.1"
    },
    "devDependencies": {
        "@types/body-parser": "^1.19.2",
        "@types/express": "^4.17.17",
        "@types/multer": "^1.4.7",
        "@types/node": "^18.13.0",
        "@typescript-eslint/eslint-plugin": "^5.49.0",
        "@typescript-eslint/parser": "^5.49.0",
        "body-parser": "^1.20.1",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.0",
        "concurrently": "^7.6.0",
        "eslint": "^8.33.0",
        "eslint-config-prettier": "^8.6.0",
        "multer": "^1.4.5-lts.1",
        "nodemon": "^2.0.20",
        "prettier": "^2.8.3",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.5"
    }
}
